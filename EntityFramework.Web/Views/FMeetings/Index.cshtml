@model IEnumerable<TestEntityFramework.Models.FMeeting>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                <!--Html.DisplayNameFor(model => model.DateTime)-->
                @Html.DisplayName("Дата проведения комиссии")
            </th>
            <th>
                <!--Html.DisplayNameFor(model => model.Place)-->
                @Html.DisplayName("Место проведения")
            </th>
            <th>
                <!--Html.DisplayNameFor(model => model.FComissionNavigation)-->
                @Html.DisplayName("Название комиссии")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FComissionNavigation.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FMeetingId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FMeetingId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FMeetingId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
